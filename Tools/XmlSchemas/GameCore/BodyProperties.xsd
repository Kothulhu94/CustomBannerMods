<?xml version="1.0" encoding="utf-8"?>
<xs:schema id="BodyProperties" xmlns="" xmlns:xs="http://www.w3.org/2001/XMLSchema"
  xmlns:msdata="urn:schemas-microsoft-com:xml-msdata">
  <xs:element name="BodyProperties" msdata:IsDataSet="true">
    <xs:complexType>
      <xs:sequence>
        <xs:element name="BodyProperty" maxOccurs="unbounded" minOccurs="0">
          <xs:complexType>
            <xs:all>
              <xs:element name="BodyPropertiesMin">
                <xs:annotation>
                  <xs:appinfo>
                    <appSpecificNote>AlwaysPreferMerge</appSpecificNote>
                  </xs:appinfo>
                </xs:annotation>
                <xs:complexType>
                  <xs:simpleContent>
                    <xs:extension base="xs:string">
                      <xs:attribute type="xs:string" name="age" use="required" />
                      <xs:attribute type="xs:string" name="weight" use="required" />
                      <xs:attribute type="xs:string" name="build" use="required" />
                      <xs:attribute type="xs:string" name="version" use="required" />
                      <xs:attribute type="xs:string" name="key" use="required" />
                    </xs:extension>
                  </xs:simpleContent>
                </xs:complexType>
              </xs:element>
              <xs:element name="BodyPropertiesMax">
                <xs:annotation>
                  <xs:appinfo>
                    <appSpecificNote>AlwaysPreferMerge</appSpecificNote>
                  </xs:appinfo>
                </xs:annotation>
                <xs:complexType>
                  <xs:simpleContent>
                    <xs:extension base="xs:string">
                      <xs:attribute type="xs:string" name="age" use="required" />
                      <xs:attribute type="xs:string" name="weight" use="required" />
                      <xs:attribute type="xs:string" name="build" use="required" />
                      <xs:attribute type="xs:string" name="version" use="required" />
                      <xs:attribute type="xs:string" name="key" use="required" />
                    </xs:extension>
                  </xs:simpleContent>
                </xs:complexType>
              </xs:element>
              <xs:element name="hair_tags" minOccurs="0">
                <xs:complexType>
                  <xs:sequence>
                    <xs:element name="hair_tag" minOccurs="0" maxOccurs="unbounded">
                      <xs:complexType>
                        <xs:attribute name="name" use="required">
                          <xs:simpleType>
                            <xs:restriction base="xs:string" />
                          </xs:simpleType>
                        </xs:attribute>
                      </xs:complexType>
                    </xs:element>
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
              <xs:element name="beard_tags" minOccurs="0">
                <xs:complexType>
                  <xs:sequence>
                    <xs:element name="beard_tag" minOccurs="0" maxOccurs="unbounded">
                      <xs:complexType>
                        <xs:attribute name="name" use="required">
                          <xs:simpleType>
                            <xs:restriction base="xs:string" />
                          </xs:simpleType>
                        </xs:attribute>
                      </xs:complexType>
                    </xs:element>
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
              <xs:element name="tattoo_tags" minOccurs="0">
                <xs:complexType>
                  <xs:sequence>
                    <xs:element name="tattoo_tag" minOccurs="0" maxOccurs="unbounded">
                      <xs:complexType>
                        <xs:attribute name="name" use="required">
                          <xs:simpleType>
                            <xs:restriction base="xs:string" />
                          </xs:simpleType>
                        </xs:attribute>
                      </xs:complexType>
                    </xs:element>
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
            </xs:all>
            <xs:attribute type="xs:string" name="id" use="required" />
          </xs:complexType>
        </xs:element>
      </xs:sequence>
    </xs:complexType>

    <!-- id attribute is unique for BodyProperty -->
    <xs:unique name="BodyProperty_unique_attribute">
      <xs:selector xpath="BodyProperty"/>
      <xs:field xpath="@id"/>
    </xs:unique>

  </xs:element>
</xs:schema>